{
    "manifest_version": 3,
    "name": "Timesheet Automation",
    "description": "Automatically generate timesheet based on Officient.io",
    "version": "2.6",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "scripting",
        "webRequest"
    ],
    "host_permissions": [
        "https://selfservice.officient.io/*"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "options_page": "options/index.html",
    "action": {
        "default_icon": {
            "16": "img/icon16.png",
            "48": "img/icon48.png",
            "128": "img/icon128.png"
        },
        "default_popup": "popup/index.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}